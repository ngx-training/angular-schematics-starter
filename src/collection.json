{
  "$schema": "../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "wdc": {
      "description": "A blank schematic.",
      "factory": "./wdc/index#wdc"
    },
    "logger": {
      "description": "Logger: Create log outputs",
      "factory": "./logger/index#logger"
    },
    "component": {
      "aliases": ["c"],
      "description": "Component own component",
      "factory": "./component-generator/index#component",
      "schema": "./component-generator/schema.json"
    },
    "update": {
      "aliases": ["u"],
      "description": "Update components to have tests",
      "factory": "./ng-update/index#updateToV2"
    }
  }
}
